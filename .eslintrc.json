{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["*.config.js", "register-service-worker.js", "*.test.js ", "node_modules/"],
    "rules": {
        "react/jsx-filename-extension": [1, {
        "extensions": [".js", ".jsx"]}
        ],
        "linebreak-style": 0,
    "react/prop-types": 0,
    "import/extensions": "off",
    "import/no-unresolved": "off"
    }
}